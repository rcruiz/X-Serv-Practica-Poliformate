        <script>
        function portal_check_pnotify() {
            $PBJQ.getJSON( "/direct/portal/notify.json", function( data ) {
                for(i=0; i<data.error.length; i++ ) {
                    $PBJQ(function(){
                        PNotify.defaults.styling = 'bootstrap3';
                        PNotify.defaults.icons = 'fontawesome4';
                        PNotify.alert({
                            title: 'Aviso',
                            text: data.error[i],
                            type: 'notice'
                        });
                    });
                }
            });
        }
        </script>

