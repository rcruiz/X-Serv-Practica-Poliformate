            <script>
            var needJQuery = true; //variables locales activada 10000
            var secondJQuery = false;
            var notJQuery = false;
            var dollarEmpty = false;
            var dollarVersion = false; //Actualizar las versiones

            // Salta la ventana. Accede por destino.
            if ( window.$ ) {
                if ( window.$.fn && window.$.fn ) {
                    dollarVersion = window.$.fn.jquery;
                } else {
                    //Inicialización, carga, lista vacia.
                    dollarVersion = 'not jQuery';
                    window.console && console.log('Dollar is defined but is not jQuery');
                    window.console && console.log($);
                     //Just have it reload this other $ afterward
                    notJQuery = true;
                }
            } else {
                //acceso a la cuarta variable.
                dollarEmpty = true;
                dollarVersion = 'not present';
                if ( window.jQuery ) {
                    // Prueba que no haya conflictos
                    window.console && console.log('tool called jQuery.noConflict()');
                }
            }
            if ( window.jQuery ) {
                tver = jQuery.fn.jquery; //Actualiza la version
                if ( tver.indexOf('1.12.') == 0 ) { //Acceso a memoria
                    window.console && console.log('Using tool jQuery ' + tver);
                    needJQuery = false; //vaciado de la variable
                } else {
                    secondJQuery = true; //Activa la segunda variable
                }
            }
            // Si esta activo la variable
            if ( needJQuery ) { //Escribe los files en función de los recursos
                document.write('script src="/library/webjars//jquery/1.12.4/jquery.min.js?version=20231114">'+'script>')
                document.write('script src="/library/webjars/jquery-migrate/1.4.1/jquery-migrate.min.js?version=20231114">'+'script>')
                document.write('script src="/library/webjars/bootstrap/3.3.7/js/bootstrap.min.js?version=20231114">'+'/script>')
                document.write('script src="/library/webjars/jquery-ui/1.12.1/jquery-ui.min.js?version=20231114">'+'/script>')
                document.write('link rel="stylesheet" href="/library/webjars/jquery-ui/1.12.1/jquery-ui.min.css?version=20231114"/>')
                // Si esta activo el toque de las pantallas en la Interface User
                if (Modernizr.touch) {
                    document.write('script type="text/javascript" src="/library/webjars/jquery-ui-touch-punch/0.2.3/jquery.ui.touch-punch.min.js?version=20231114">'+'/script>')
                }
                window.console && console.log('Portal scripts loaded JQ+MI+BS+UI');
            //Si no esta activo el needJQuery, haces una migracion de errores,
            //quitas los pop-up, puede que este activa la UI
            } else {
                if (typeof jQuery.migrateWarnings == 'undefined') {
                    //Activas el script a traves del fichero
                    document.write('script type="text/javascript" src="/library/webjars/jquery-migrate/1.4.1/jquery-migrate.min.js?version=20231114">'+'/script>')
                    window.console && console.log('Portal adding jQuery migrate');
                }
                if ( typeof jQuery.fn.popover == 'undefined') {
                    document.write('script type="text/javascript" src="/library/webjars/bootstrap/3.3.7/js/bootstrap.min.js?version=20231114">'+'/script>')
                    window.console && console.log('Portal adding Bootstrap');
                }
                if (typeof jQuery.ui == 'undefined') {
                    document.write('script type="text/javascript" src="/library/webjars/jquery-ui/1.12.1/jquery-ui.min.js?version=20231114">'+'/script>')
                    document.write('link rel="stylesheet" href="/library/webjars/jquery-ui/1.12.1/jquery-ui.min.css?version=20231114"/>')
                    window.console && console.log('Portal adding jQuery UI');
                }
                // Si esta activo el raton y ademas el protipo por sensor de movimiento
                if (typeof jQuery.ui == 'undefined' || (typeof jQuery.ui.mouse != 'undefined' && typeof jQuery.ui.mouse.prototype._touchStart == 'undefined')) {
                    document.write('script type="text/javascript" src="/library/webjars/jquery-ui-touch-punch/0.2.3/jquery.ui.touch-punch.min.js?version=20231114">'+'/script>')
                    //Accedes a la UI a traves del dispositivo.
                    window.console && console.log('Portal adding jQuery UI Touch Punch for touch device support');
                }
            }
            </script>


