        <script>

        function paSystemJQueryOK() {
            //Funcion que permite trabajar con pages PULL en versiones de JQuery
            if (!window.jQuery) {
                /* No hay ventana de JQuery*/
                return false;
            }
            //Comprueba la version a traves del parseo de enteros
            var version_numbers = jQuery.fn.jquery.split('.');
            /* >= 1.9+ */
            return (parseInt(version_numbers[0]) > 1) || (parseInt(version_numbers[0]) == 1 && parseInt(version_numbers[1]) >= 9);
        }
        //si esta activo escribe unos ficheros a partir de los script
        if (paSystemJQueryOK()) {
            document.write('script src="/pasystem-tool/scripts/pasystem.js?version;20231114">/script>');
            document.write('script>includeWebjarLibrary(\'featherlight\')/script>');
            // Actualizacion de la variable del sistema cuando esta cargada
            var pasystem = pasystem || {'loaded': true};
        } else {
            //si no estas logueado carga desde el JQuery no soportado
            console.log("PASystem not loaded for this page because jQuery is missing or unsupported.");
        }
        </script>

